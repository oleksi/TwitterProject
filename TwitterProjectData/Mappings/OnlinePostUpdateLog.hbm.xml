<?xml version="1.0" encoding="utf-8" ?> 

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="TwitterProjectModel" 
                   namespace="TwitterProjectModel">
 
  <class name="OnlinePostUpdateLog" table="OnlinePostUpdateLogs">
		<id name="Id" column="OnlinePostUpdateLogId">
			<generator class="identity"></generator>
		</id>
		<many-to-one class="Model" name="Model" column="ModelId" not-null="true" />
		<many-to-one class="OnlinePostUpdate" name="OnlinePostUpdate" column="OnlinePostUpdateId" not-null="true" />
		<property name="LastPublishedDate" not-null="true"></property>
	</class>
  
</hibernate-mapping>